# Hysteria2 Client Configuration Template
# This template is used to generate client configurations

# Server Configuration
SERVER={{server}}
AUTH_KEY={{auth_key}}
SNI={{sni}}

# Connection Settings
UPLOAD_SPEED={{upload_speed}}
DOWNLOAD_SPEED={{download_speed}}

# Corporate Network Routes (Split Tunneling)
CORPORATE_NETWORKS="192.168.0.0/16,10.0.0.0/8"

# Anti-censorship Settings
MASQUERADE_URL=https://dl.google.com
MASQUERADE_TYPE=proxy

# Security Settings
TLS_INSECURE=false
UDP_PREFER=true

# Performance Settings
UDP_IDLE_TIMEOUT=30s
MAX_CONNECTIONS=1000

# Logging
LOG_LEVEL=info